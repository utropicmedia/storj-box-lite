  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "is-ci || husky install",
    "dev": "vite --open",
    "format": "prettier --write .",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint src/**/* --ext js --ext ts --ext tsx",
