    "@typescript-eslint/parser": "4.22.0",
    "@vitejs/plugin-react-refresh": "1.3.2",
    "autoprefixer": "10.2.5",
    "cypress": "7.1.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.0.0",
    "eslint-plugin-unicorn": "29.0.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "postcss": "8.2.10",
    "prettier": "2.2.1",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "21.0.0",
    "tailwindcss": "2.1.1",
    "typescript": "4.2.4",
    "vite": "2.1.5",
    "vite-plugin-pwa": "0.7.1",
    "vite-tsconfig-paths": "2.5.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{html,js,ts,tsx,css,md,json}": "prettier --write"
  },
  "browserslist": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48"
