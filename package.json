    "@fortawesome/react-fontawesome": "^0.1.14",
    "classnames": "^2.3.1",
    "firebase": "^8.4.1",
    "formik": "^2.2.6",
    "framer-motion": "^4.1.3",
    "is-ci": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "^3.0.3",
    "react-query": "3.13.7",
    "react-router-dom": "5.2.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.2",
